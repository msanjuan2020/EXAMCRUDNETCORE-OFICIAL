@model EXAMCRUDNETCORE.Models.Empleado
@{
    ViewData["Title"] = "Eliminar";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1 class="text-danger">Esta seguro de eliminar el emplleado?</h1>

<div class="row">
    <div class="col-sm-4">
        <form class="form-control" asp-action="Eliminar" asp-controller="Home" method="post">

            <input type="hidden" asp-for="IdEmpleado" />

            <div class="mb-2">
                <label class="form-label" asp-for="Nombres">Nombres: </label>
                <input class="form-control formform-control-sm" asp-for="Nombres" type="text" disabled />
            </div>
            <div class="mb-2">
                <label class="form-label" asp-for="Apellidos">Apellidos: </label>
                <input class="form-control formform-control-sm" asp-for="Apellidos" type="text" disabled />
            </div>
            <div class="mb-2">
                <label class="form-label" asp-for="Correo">Correo: </label>
                <input class="form-control formform-control-sm" asp-for="Correo" type="text" disabled />
            </div>
            <div class="mb-2">
                <label class="form-label" asp-for="Telefono">Telefono: </label>
                <input class="form-control formform-control-sm" asp-for="Telefono" type="text" disabled />
            </div>
            <div class="mb-3">
                <label class="form-label" asp-for="IdCargo">Cargo: </label>
                <input class="form-control formform-control-sm" asp-for="oCargo.Descripcion" type="text" disabled />
            </div>

            <div class="mb-2">

                <button class="btn btn-danger btn-sm  " type="submit" asp-action="Index"> Eliminar </button>
                <a class="btn btn-dark btn-sm" asp-action="Index" asp-controller="Home"> Volver </a>


            </div>
        </form>
    </div>
</div>